{
  "default": true,
  "prefix": "pencil",
  "alias": "pl",
  "tags": {

    "html": {
      "alias": [
        "html:?lang"
      ]
    },

    "favicon": {
      "alias": [
        "favicon:*"
      ]
    },

    "form": {
      "alias": [
        "form:?method"
      ]
    },

    "input": {
      "alias": [
        "input:?type:?name?:?value?"
      ]
    },

    "button": {
      "alias": [
        "button:?type"
      ]
    },

    "option": {
      "alias": [
        "option:?value:?label?"
      ]
    },

    "style": {},

    "style.cdn": {
      "alias": [
        "style:?library"
      ]
    },

    "script": {},

    "script.manager": {},

    "script.cdn": {
      "alias": [
        "script:?library",
        "script:?library:*"
      ]
    },

    "meta": {
      "alias": [
        "meta:?type"
      ]
    },

    "noframe": {},

    "container.container": {
      "alias": [
        "container"
      ]
    },

    "container.viewport": {
      "alias": [
        "viewport"
      ]
    },

    "panel.panel": {
      "alias": [
        "panel"
      ]
    },

    "tab.panel": {
      "alias": [
        "tabs"
      ]
    },

    "captcha": {
      "alias": [
        "captcha:?key?"
      ]
    },

    "extension": {
      "alias": [
        "ext:?type?:?username?:?extension?"
      ]
    },

    "template": {}

  }

}