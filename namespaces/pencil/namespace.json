{
  "default": true,
  "prefix": "pencil",
  "alias": "pl",
  "tags": {

    "pencil.html": {
      "alias": [
        "html:?lang"
      ]
    },

    "pencil.favicon": {
      "alias": [
        "favicon:*"
      ]
    },

    "pencil.form": {
      "alias": [
        "form:?method"
      ]
    },

    "pencil.input": {
      "alias": [
        "input:?type:?name?:?value?"
      ]
    },

    "pencil.button": {
      "alias": [
        "button:?type"
      ]
    },

    "pencil.option": {
      "alias": [
        "option:?value:?label?"
      ]
    },

    "pencil.style": {},

    "pencil.style.cdn": {
      "alias": [
        "style:?library"
      ]
    },

    "pencil.script": {},

    "pencil.script.manager": {},

    "pencil.script.cdn": {
      "alias": [
        "script:?library"
      ]
    },

    "pencil.meta": {
      "alias": [
        "meta:?type"
      ]
    },

    "pencil.noframe": {},

    "pencil.container.container": {
      "alias": [
        "container"
      ]
    },

    "pencil.container.viewport": {
      "alias": [
        "viewport"
      ]
    },

    "pencil.panel.panel": {
      "alias": [
        "panel"
      ]
    },

    "pencil.tab.panel": {
      "alias": [
        "tabs"
      ]
    },

    "pencil.captcha": {
      "alias": [
        "captcha:?key?"
      ]
    },

    "pencil.extension": {
      "alias": [
        "ext:?type?:?username?:?extension?"
      ]
    }

  }

}